  <script>
    // Mobile menu toggle
    document.addEventListener('DOMContentLoaded', function() {
      const menuButton = document.querySelector('.navbar-menu-button');
      const menu = document.querySelector('.navbar-menu-dark');
      
      if (menuButton && menu) {
        menuButton.addEventListener('click', function() {
          menuButton.classList.toggle('w--open');
          menu.classList.toggle('w--open');
        });
      }

      // Desktop dropdowns
      const dropdowns = document.querySelectorAll('.dropdown-dark');
      dropdowns.forEach(dropdown => {
        const toggle = dropdown.querySelector('.dropdown-toggle-dark');
        
        if (toggle) {
          toggle.addEventListener('click', function(e) {
            e.stopPropagation();
            
            // Close other dropdowns
            dropdowns.forEach(otherDropdown => {
              if (otherDropdown !== dropdown) {
                otherDropdown.classList.remove('w--open');
              }
            });
            
            // Toggle current dropdown
            dropdown.classList.toggle('w--open');
          });
        }
      });

      // Close dropdowns when clicking outside
      document.addEventListener('click', function(event) {
        if (!event.target.closest('.dropdown-dark')) {
          dropdowns.forEach(dropdown => {
            dropdown.classList.remove('w--open');
          });
        }
      });

      // Close mobile menu when clicking a link
      const navLinks = document.querySelectorAll('.nav-link-dark, .dropdown-link-dark');
      navLinks.forEach(link => {
        link.addEventListener('click', function() {
          if (window.innerWidth <= 991) {
            menuButton?.classList.remove('w--open');
            menu?.classList.remove('w--open');
          }
        });
      });

      // Form submission
      const contactForm = document.getElementById('contactForm');
      if (contactForm) {
        contactForm.addEventListener('submit', function(e) {
          e.preventDefault();
          alert('Thank you for your interest! We\'ll contact you within 1 business day to schedule your demo.');
          this.reset();
        });
      }

      // FAQ Accordion
      const faqItems = document.querySelectorAll('.faq-item');
      faqItems.forEach(item => {
        const question = item.querySelector('.faq-question');

        if (question) {
          question.addEventListener('click', function() {
            // Close other FAQs
            faqItems.forEach(otherItem => {
              if (otherItem !== item) {
                otherItem.classList.remove('active');
              }
            });

            // Toggle current FAQ
            item.classList.toggle('active');
          });
        }
      });

      // ROI Calculator
      const caseValue = document.getElementById('caseValue');
      const currentConsults = document.getElementById('currentConsults');
      const showUpRate = document.getElementById('showUpRate');
      const newPatients = document.getElementById('newPatients');
      const revenueIncrease = document.getElementById('revenueIncrease');
      const annualGrowth = document.getElementById('annualGrowth');

      function calculateROI() {
        const avgCaseValue = parseInt(caseValue.value) || 25000;
        const currentMonthly = parseInt(currentConsults.value) || 5;
        const currentShowUp = parseInt(showUpRate.value) || 60;

        // DGP typically adds 15-20 consultations per month with 75% show-up
        const additionalConsults = 20;
        const dgpShowUpRate = 75;

        // Calculate new patients
        const newPatientsPerMonth = Math.floor(additionalConsults * (dgpShowUpRate / 100));

        // Calculate revenue increase
        const monthlyRevenue = newPatientsPerMonth * avgCaseValue;
        const annualRevenue = monthlyRevenue * 12;

        // Update display
        newPatients.textContent = newPatientsPerMonth + '+';
        revenueIncrease.textContent = '$' + monthlyRevenue.toLocaleString();
        annualGrowth.textContent = '$' + (annualRevenue / 1000000).toFixed(1) + 'M+';
      }

      if (caseValue && currentConsults && showUpRate) {
        caseValue.addEventListener('input', calculateROI);
        currentConsults.addEventListener('input', calculateROI);
        showUpRate.addEventListener('input', calculateROI);

        // Initial calculation
        calculateROI();
      }
    });
  </script>

