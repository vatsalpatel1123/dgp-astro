    .calculator-cta a {
      display: inline-block;
      padding: 18px 48px;
      background: var(--dark-slate-blue);
      color: var(--white);
      border-radius: 14px;
      text-decoration: none;
      font-weight: 700;
      font-size: 18px;
      transition: all 0.3s ease;
      box-shadow: 0 6px 20px rgba(8, 57, 129, 0.3);
