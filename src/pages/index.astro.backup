    .calculator-field {
      display: flex;
      flex-direction: column;
    }

    .calculator-field label {
      font-size: 16px;
      font-weight: 700;
      color: var(--black);
      margin-bottom: 12px;
    }

    .calculator-field input {
      padding: 16px 20px;
      border: 2px solid #e5e7eb;
      border-radius: 12px;
      font-size: 18px;
      font-weight: 600;
      transition: all 0.3s ease;
    }

    .calculator-field input:focus {
      outline: none;
      border-color: var(--dark-slate-blue);
      box-shadow: 0 0 0 4px rgba(8, 57, 129, 0.1);
    }

    .calculator-results {
      background: var(--natural);
      padding: 40px;
      border-radius: 20px;
      text-align: center;
    }

    .calculator-results h3 {
      font-size: 24px;
      font-weight: 700;
      color: var(--black);
      margin-bottom: 24px;
