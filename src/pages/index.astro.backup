    .result-label {
      font-size: 14px;
      font-weight: 600;
      color: var(--black);
      text-transform: uppercase;
      letter-spacing: 0.5px;
      margin-bottom: 8px;
    }

    .result-value {
      font-size: 36px;
      font-weight: 800;
      color: var(--dark-slate-blue);
