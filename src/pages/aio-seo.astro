---
// aio-seo.astro - Updated
import '../css/main.css';
import Navigation from '../components/Navigation.astro';
import FooterBar from '../components/FooterBar.astro';
---
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>All-In-One SEO | Dentistry Growth Partners</title>
  <meta name="description" content="Dominate local search rankings and get recommended by ChatGPT & other AI Models to become the #1 choice for multi-implants and dental help in your area.">
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700;800&display=swap" rel="stylesheet">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@tabler/icons-webfont@latest/tabler-icons.min.css">
  <link rel="icon" type="image/png" href="/636f4050ef41c9124a12cbba/dgp-logo1.png" />
  <link rel="shortcut icon" type="image/png" href="/636f4050ef41c9124a12cbba/dgp-logo1.png" />
  
  <style>
    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
    }

    :root {
      --primary-light: #EDEDE8;
      --primary-dark: #051023;
      --primary-blue: #07387D;
      --accent-blue: #248EEF;
      --white: #ffffff;
      --black: #1d1d1d;
      --grey: #f5f5f5;
      --dim-grey: #646464;
    }

    body {
      font-family: 'Inter', sans-serif;
      background: var(--white);
      color: var(--black);
      line-height: 1.6;
      overflow-x: hidden;
    }

    .section-container {
      max-width: 1400px;
      margin: 0 auto;
      padding: 0 24px;
    }

    /* HERO SECTION */
    .hero-section {
      min-height: 85vh;
      display: flex;
      align-items: center;
      justify-content: center;
      position: relative;
      overflow: hidden;
      background: linear-gradient(135deg, var(--primary-dark) 0%, var(--primary-blue) 50%, var(--accent-blue) 100%);
      padding: 160px 24px 100px;
    }

    /* Animated gradient orbs */
    .hero-section::before {
      content: '';
      position: absolute;
      top: -150px;
      right: -150px;
      width: 500px;
      height: 500px;
      background: radial-gradient(circle, rgba(36, 142, 239, 0.25) 0%, transparent 70%);
      border-radius: 50%;
      animation: floatOrb 20s ease-in-out infinite;
      z-index: 1;
    }

    .hero-section::after {
      content: '';
      position: absolute;
      bottom: -100px;
      left: -100px;
      width: 400px;
      height: 400px;
      background: radial-gradient(circle, rgba(36, 142, 239, 0.2) 0%, transparent 70%);
      border-radius: 50%;
      animation: floatOrb 25s ease-in-out infinite reverse;
      z-index: 1;
    }

    @keyframes floatOrb {
      0%, 100% {
        transform: translate(0, 0) scale(1);
      }
      33% {
        transform: translate(30px, -30px) scale(1.1);
      }
      66% {
        transform: translate(-20px, 20px) scale(0.9);
      }
    }

    .hero-background-overlay {
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      background-image: url('https://images.unsplash.com/photo-1551076805-e1869033e561?w=1920&q=80');
      background-size: cover;
      background-position: center;
      opacity: 0.08;
      z-index: 0;
    }

    /* Dot pattern overlay */
    .hero-background-overlay::after {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      background-image: radial-gradient(circle, rgba(255, 255, 255, 0.05) 1px, transparent 1px);
      background-size: 30px 30px;
      z-index: 1;
    }

    .hero-container {
      max-width: 1100px;
      width: 100%;
      margin: 0 auto;
      position: relative;
      z-index: 10;
      display: flex;
      flex-direction: column;
      align-items: center;
      justify-content: center;
      text-align: center;
    }

    .hero-content {
      text-align: center;
      position: relative;
    }

    .hero-section h1 {
      font-size: clamp(42px, 5.5vw, 62px);
      font-weight: 800;
      color: #ffffff;
      line-height: 1.15;
      margin: 0 0 24px 0;
      letter-spacing: -1.8px;
      text-shadow: 0 4px 20px rgba(0, 0, 0, 0.3);
      position: relative;
    }

    .hero-subtitle {
      font-size: clamp(20px, 2.2vw, 24px);
      color: rgba(255, 255, 255, 0.95);
      line-height: 1.6;
      margin-bottom: 0;
      font-weight: 600;
    }

    /* WHY IT MATTERS SECTION */
    .why-matters-section {
      padding: 120px 24px;
      background: var(--white);
    }

    .section-header {
      max-width: 900px;
      margin: 0 auto 60px;
      text-align: center;
    }

    .section-title {
      font-size: clamp(32px, 4vw, 48px);
      font-weight: 800;
      color: var(--primary-blue);
      margin-bottom: 20px;
      line-height: 1.2;
      letter-spacing: -1px;
    }

    .why-matters-grid {
      display: grid;
      grid-template-columns: repeat(2, 1fr);
      gap: 32px;
      max-width: 1200px;
      margin: 0 auto;
    }

    .benefit-card {
      background: var(--white);
      border: 3px solid var(--grey);
      border-radius: 24px;
      padding: 48px 40px;
      transition: all 0.4s ease;
    }

    .benefit-card:hover {
      transform: translateY(-8px);
      box-shadow: 0 20px 60px rgba(0, 0, 0, 0.15);
      border-color: var(--accent-blue);
    }

    .benefit-badge {
      display: inline-flex;
      align-items: center;
      gap: 8px;
      background: linear-gradient(135deg, var(--primary-blue), var(--accent-blue));
      color: var(--white);
      padding: 10px 20px;
      border-radius: 50px;
      font-size: 13px;
      font-weight: 700;
      text-transform: uppercase;
      letter-spacing: 0.5px;
      margin-bottom: 24px;
    }

    .benefit-badge i {
      font-size: 16px;
    }

    .benefit-card h3 {
      font-size: 24px;
      font-weight: 800;
      color: var(--primary-blue);
      margin-bottom: 16px;
      letter-spacing: -0.5px;
    }

    .benefit-card p {
      font-size: 16px;
      color: var(--dim-grey);
      line-height: 1.7;
      margin: 0;
    }

    /* FEATURES SECTION */
    .features-section {
      padding: 120px 24px;
      background: var(--primary-light);
    }

    .features-intro {
      max-width: 900px;
      margin: 0 auto 60px;
      text-align: center;
    }

    .features-intro p {
      font-size: 19px;
      color: var(--dim-grey);
      line-height: 1.7;
      margin-top: 20px;
    }

    .features-grid {
      display: grid;
      grid-template-columns: repeat(3, 1fr);
      gap: 32px;
      max-width: 1300px;
      margin: 0 auto;
    }

    .feature-card {
      background: var(--white);
      border: 3px solid var(--grey);
      border-radius: 20px;
      padding: 40px 32px;
      transition: all 0.4s ease;
      position: relative;
      overflow: hidden;
    }

    .feature-card::before {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: linear-gradient(135deg, transparent 0%, rgba(36, 142, 239, 0.05) 100%);
      opacity: 0;
      transition: opacity 0.4s ease;
    }

    .feature-card:hover {
      border-color: var(--accent-blue);
      transform: translateY(-8px);
      box-shadow: 0 16px 48px rgba(0, 0, 0, 0.12);
    }

    .feature-card:hover::before {
      opacity: 1;
    }

    .feature-icon {
      width: 70px;
      height: 70px;
      background: linear-gradient(135deg, var(--primary-blue), var(--accent-blue));
      border-radius: 16px;
      display: flex;
      align-items: center;
      justify-content: center;
      font-size: 32px;
      color: var(--white);
      margin-bottom: 24px;
      transition: all 0.4s ease;
    }

    .feature-card:hover .feature-icon {
      transform: scale(1.1) rotate(5deg);
    }

    .feature-card h3 {
      font-size: 22px;
      font-weight: 800;
      color: var(--primary-blue);
      margin-bottom: 12px;
      letter-spacing: -0.3px;
    }

    .feature-card p {
      font-size: 15px;
      color: var(--dim-grey);
      line-height: 1.7;
      margin: 0;
    }

    /* PROCESS SECTION */
    .process-section {
      padding: 120px 24px;
      background: var(--white);
    }

    .process-timeline {
      max-width: 1000px;
      margin: 60px auto 0;
      position: relative;
    }

    .process-timeline::before {
      content: '';
      position: absolute;
      left: 50%;
      top: 0;
      bottom: 0;
      width: 3px;
      background: linear-gradient(180deg, var(--primary-blue), var(--accent-blue));
      transform: translateX(-50%);
    }

    .process-step {
      display: grid;
      grid-template-columns: 1fr 80px 1fr;
      gap: 40px;
      margin-bottom: 60px;
      align-items: center;
    }

    .process-step:last-child {
      margin-bottom: 0;
    }

    .step-content-left,
    .step-content-right {
      background: var(--white);
      border: 3px solid var(--grey);
      border-radius: 20px;
      padding: 36px 32px;
      transition: all 0.4s ease;
    }

    .step-content-left:hover,
    .step-content-right:hover {
      border-color: var(--accent-blue);
      transform: translateY(-4px);
      box-shadow: 0 12px 32px rgba(0, 0, 0, 0.1);
    }

    .step-content-left {
      text-align: right;
    }

    .step-content-right {
      text-align: left;
    }

    .step-number {
      width: 80px;
      height: 80px;
      background: linear-gradient(135deg, var(--primary-blue), var(--accent-blue));
      border: 4px solid var(--white);
      border-radius: 50%;
      display: flex;
      align-items: center;
      justify-content: center;
      font-size: 32px;
      font-weight: 800;
      color: var(--white);
      position: relative;
      z-index: 10;
      box-shadow: 0 8px 24px rgba(0, 0, 0, 0.2);
    }

    .step-content-left h3,
    .step-content-right h3 {
      font-size: 24px;
      font-weight: 800;
      color: var(--primary-blue);
      margin-bottom: 12px;
      letter-spacing: -0.3px;
    }

    .step-content-left p,
    .step-content-right p {
      font-size: 15px;
      color: var(--dim-grey);
      line-height: 1.7;
      margin: 0;
    }

    /* FINAL CTA */
    .final-cta-section {
      padding: 100px 24px;
      background: linear-gradient(135deg, var(--primary-blue) 0%, var(--accent-blue) 100%);
      position: relative;
      overflow: hidden;
    }

    .final-cta-content {
      max-width: 900px;
      margin: 0 auto;
      text-align: center;
      position: relative;
      z-index: 10;
    }

    .final-cta-icon {
      width: 100px;
      height: 100px;
      background: rgba(255, 255, 255, 0.15);
      border: 3px solid var(--white);
      border-radius: 50%;
      display: flex;
      align-items: center;
      justify-content: center;
      margin: 0 auto 32px;
      font-size: 48px;
      color: var(--white);
    }

    .final-cta-section h2 {
      font-size: clamp(32px, 4vw, 48px);
      font-weight: 800;
      color: var(--white);
      margin-bottom: 24px;
      line-height: 1.2;
      letter-spacing: -1px;
    }

    .final-cta-text {
      font-size: 20px;
      color: rgba(255, 255, 255, 0.95);
      line-height: 1.7;
      margin-bottom: 40px;
      max-width: 800px;
      margin-left: auto;
      margin-right: auto;
    }

    .final-cta-button {
      display: inline-flex;
      align-items: center;
      gap: 12px;
      padding: 20px 48px;
      background: var(--white);
      color: var(--primary-blue);
      font-size: 18px;
      font-weight: 700;
      border-radius: 14px;
      text-decoration: none;
      transition: all 0.3s ease;
      border: 3px solid var(--white);
      box-shadow: 0 8px 24px rgba(255, 255, 255, 0.3);
    }

    .final-cta-button:hover {
      background: transparent;
      color: var(--white);
      transform: translateY(-4px);
      box-shadow: 0 12px 32px rgba(255, 255, 255, 0.4);
    }

    .final-cta-button i {
      font-size: 22px;
    }

    /* FLOATING CTA */
    .floating-cta {
      position: fixed;
      bottom: 32px;
      right: 32px;
      display: flex;
      flex-direction: column;
      gap: 16px;
      z-index: 1000;
    }

    .floating-btn {
      display: flex;
      align-items: center;
      gap: 12px;
      padding: 16px 24px;
      background: var(--primary-blue);
      color: var(--white);
      border-radius: 50px;
      text-decoration: none;
      font-weight: 700;
      font-size: 15px;
      box-shadow: 0 8px 24px rgba(0, 0, 0, 0.2);
      transition: all 0.3s ease;
    }

    .floating-btn:hover {
      transform: translateY(-4px);
      box-shadow: 0 12px 32px rgba(0, 0, 0, 0.3);
      background: var(--accent-blue);
    }

    /* RESPONSIVE */
    @media (max-width: 991px) {
      .hero-section {
        padding: 140px 24px 80px;
        min-height: 75vh;
      }

      .why-matters-grid, .features-grid {
        grid-template-columns: repeat(2, 1fr);
        gap: 24px;
      }

      .process-timeline::before {
        display: none;
      }

      .process-step {
        grid-template-columns: 1fr;
        gap: 24px;
        margin-bottom: 40px;
      }

      .step-content-left, .step-content-right {
        text-align: left;
      }

      .step-number {
        margin: 0 auto;
      }

      .why-matters-section, .features-section, .process-section, .final-cta-section {
        padding: 80px 24px;
      }
    }

    @media (max-width: 767px) {
      .hero-section {
        padding: 130px 20px 60px;
        min-height: 70vh;
      }

      .hero-section h1 {
        font-size: clamp(32px, 7vw, 44px);
        margin-bottom: 20px;
      }

      .hero-subtitle {
        font-size: 18px;
      }

      .why-matters-grid, .features-grid {
        grid-template-columns: 1fr;
        gap: 20px;
      }

      .feature-card, .benefit-card {
        padding: 36px 28px;
      }

      .feature-icon {
        width: 60px;
        height: 60px;
        font-size: 28px;
      }

      .benefit-card h3 {
        font-size: 22px;
      }

      .final-cta-icon {
        width: 80px;
        height: 80px;
        font-size: 40px;
      }

      .final-cta-text {
        font-size: 17px;
      }

      .final-cta-button {
        padding: 18px 36px;
        font-size: 16px;
        width: 100%;
        justify-content: center;
      }

      .why-matters-section, .features-section, .process-section, .final-cta-section {
        padding: 60px 20px;
      }

      .floating-cta {
        bottom: 20px;
        right: 20px;
        gap: 12px;
      }

      .floating-btn {
        padding: 14px 20px;
        font-size: 14px;
      }

      .floating-btn span {
        display: none;
      }
    }

    @media (max-width: 479px) {
      .hero-section {
        padding: 120px 16px 50px;
      }

      .hero-section h1 {
        font-size: clamp(28px, 8vw, 38px);
      }

      .feature-card h3 {
        font-size: 20px;
      }

      .benefit-card h3 {
        font-size: 20px;
      }

      .final-cta-section h2 {
        font-size: 28px;
      }
    }
  </style>
</head>
<body>
  <Navigation />
  
  <!-- HERO SECTION -->
  <section class="hero-section">
    <div class="hero-background-overlay"></div>

    <div class="hero-container">
      <div class="hero-content">
        <h1>All-In-One SEO</h1>

        <p class="hero-subtitle">
          Dominate local search rankings and get recommended by ChatGPT & other AI Models to become the #1 choice for multi-implants and dental help in your area.
        </p>
      </div>
    </div>
  </section>

  <!-- WHY IT MATTERS SECTION -->
  <section class="why-matters-section">
    <div class="section-container">
      <div class="section-header">
        <h2 class="section-title">Why Google + AI Search Rankings is Critical for Implant Practices</h2>
      </div>

      <div class="why-matters-grid">
        
        <div class="benefit-card">
          <div class="benefit-badge">
            <i class="ti ti-clock-hour-4"></i>
            24 Hours
          </div>
          <h3>76% of People Who Search for a Local Business Visit Within 24 Hours</h3>
          <p>
            3 quarters of people searching for "a dentist near me" visit the practice they find within one day. If you're not visible, you're missing out on the highest-intent, ready to book leads in dentistry.
          </p>
        </div>

        <div class="benefit-card">
          <div class="benefit-badge">
            <i class="ti ti-trophy"></i>
            First Page
          </div>
          <h3>First Page of Google Captures 92% of All Search Traffic</h3>
          <p>
            The first page of google captures virtually all search traffic. If you're not ranking in the top 10, your chances of a new patient finding you is close to none.
          </p>
        </div>

        <div class="benefit-card">
          <div class="benefit-badge">
            <i class="ti ti-robot"></i>
            AI Search
          </div>
          <h3>800% Increase in AI Search Usage Since the Start of the Year</h3>
          <p>
            Search behaviors are actively changing in favor of AI models. 42% of users now start their search on an LLM like ChatGPT, Perplexity, or Deepseek rather than traditional search. Be one of the first to be a recommended option for dentistry in your area and dominate organic referrals from AI.
          </p>
        </div>

        <div class="benefit-card">
          <div class="benefit-badge">
            <i class="ti ti-target"></i>
            Quality
          </div>
          <h3>SEO Delivers High-Quality Leads</h3>
          <p>
            SEO captures patients actively searching for a solution right now. Capture the highest-intent leads by being there in search results when they are seeking an answer.
          </p>
        </div>

      </div>
    </div>
  </section>

  <!-- FEATURES SECTION -->
  <section class="features-section">
    <div class="section-container">
      <div class="features-intro">
        <h2 class="section-title">A Complete SEO Solution</h2>
        <p>
          3-5x organic web traffic & AI visibility. Get recommended by AI which is set to dominate search inquiries by 2030. Tangible Results in just 30 days.
        </p>
      </div>

      <div class="features-grid">
        
        <div class="feature-card">
          <div class="feature-icon">
            <i class="ti ti-map-pin"></i>
          </div>
          <h3>Local SEO Optimization</h3>
          <p>
            Google Business Profile management, local citations, map rankings.
          </p>
        </div>

        <div class="feature-card">
          <div class="feature-icon">
            <i class="ti ti-file-search"></i>
          </div>
          <h3>On-Page SEO</h3>
          <p>
            Website optimization, keyword targeting, content strategy.
          </p>
        </div>

        <div class="feature-card">
          <div class="feature-icon">
            <i class="ti ti-pencil"></i>
          </div>
          <h3>Content Creation</h3>
          <p>
            SEO-optimized blog posts and service pages.
          </p>
        </div>

        <div class="feature-card">
          <div class="feature-icon">
            <i class="ti ti-code"></i>
          </div>
          <h3>Technical SEO</h3>
          <p>
            Site speed, mobile optimization, schema markup.
          </p>
        </div>

        <div class="feature-card">
          <div class="feature-icon">
            <i class="ti ti-link"></i>
          </div>
          <h3>Link Building</h3>
          <p>
            High-quality backlinks from dental industry sources.
          </p>
        </div>

        <div class="feature-card">
          <div class="feature-icon">
            <i class="ti ti-chart-line"></i>
          </div>
          <h3>Monthly Reporting</h3>
          <p>
            Track rankings, traffic, and conversions.
          </p>
        </div>

      </div>
    </div>
  </section>

  <!-- PROCESS SECTION -->
  <section class="process-section">
    <div class="section-container">
      <div class="section-header">
        <h2 class="section-title">How We Get You to #1</h2>
      </div>

      <div class="process-timeline">
        
        <div class="process-step">
          <div class="step-content-left">
            <h3>Audit</h3>
            <p>
              Complete analysis of your current SEO performance.
            </p>
          </div>
          <div class="step-number">1</div>
          <div></div>
        </div>

        <div class="process-step">
          <div></div>
          <div class="step-number">2</div>
          <div class="step-content-right">
            <h3>Strategy</h3>
            <p>
              Custom SEO roadmap tailored to your market.
            </p>
          </div>
        </div>

        <div class="process-step">
          <div class="step-content-left">
            <h3>Implementation</h3>
            <p>
              Execute on-page, off-page, and technical SEO.
            </p>
          </div>
          <div class="step-number">3</div>
          <div></div>
        </div>

        <div class="process-step">
          <div></div>
          <div class="step-number">4</div>
          <div class="step-content-right">
            <h3>Optimization</h3>
            <p>
              Ongoing monitoring and continuous improvement.
            </p>
          </div>
        </div>

      </div>
    </div>
  </section>

  <!-- FINAL CTA SECTION -->
  <section class="final-cta-section">
    <div class="final-cta-content">
      <div class="final-cta-icon">
        <i class="ti ti-chart-arrows-vertical"></i>
      </div>
      
      <h2>Ready to Turn Search Into Your Next Growth Channel?</h2>
      
      <p class="final-cta-text">
        Stop losing patients to competitors who rank above you. Rank top 3 and turn your website into a patient-generating machine for years to come.
      </p>
      
      <a href="/book-a-call" class="final-cta-button">
        <i class="ti ti-calendar-event"></i>
        Get Started Now
      </a>
    </div>
  </section>

  <FooterBar />

  <!-- FLOATING CTA -->
  <div class="floating-cta">
    <a href="/book-a-call" class="floating-btn">
      <i class="ti ti-calendar-event" style="font-size: 24px;"></i>
      <span>Let's Talk</span>
    </a>
    <a href="tel:+16505578269" class="floating-btn">
      <i class="ti ti-phone" style="font-size: 24px;"></i>
      <span>Call Now</span>
    </a>
  </div>

  <script>
    document.addEventListener('DOMContentLoaded', function() {
      const observerOptions = {
        threshold: 0.1,
        rootMargin: '0px 0px -50px 0px'
      };

      const observer = new IntersectionObserver((entries) => {
        entries.forEach(entry => {
          if (entry.isIntersecting) {
            const target = entry.target as HTMLElement;
            target.style.opacity = '1';
            target.style.transform = 'translateY(0)';
          }
        });
      }, observerOptions);

      document.querySelectorAll('.feature-card').forEach((card, index) => {
        const element = card as HTMLElement;
        element.style.opacity = '0';
        element.style.transform = 'translateY(30px)';
        element.style.transition = `opacity 0.6s ease ${index * 0.1}s, transform 0.6s ease ${index * 0.1}s`;
        observer.observe(card);
      });

      document.querySelectorAll('.benefit-card').forEach((card, index) => {
        const element = card as HTMLElement;
        element.style.opacity = '0';
        element.style.transform = 'translateY(30px)';
        element.style.transition = `opacity 0.6s ease ${index * 0.15}s, transform 0.6s ease ${index * 0.15}s`;
        observer.observe(card);
      });

      document.querySelectorAll('.process-step').forEach((step, index) => {
        const element = step as HTMLElement;
        element.style.opacity = '0';
        element.style.transform = 'translateY(30px)';
        element.style.transition = `opacity 0.6s ease ${index * 0.2}s, transform 0.6s ease ${index * 0.2}s`;
        observer.observe(step);
      });
    });
  </script>

</body>
</html>