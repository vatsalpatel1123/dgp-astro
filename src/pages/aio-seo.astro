---
// aio-seo.astro
import '../css/main.css';
import Navigation from '../components/Navigation.astro';
import FooterBar from '../components/FooterBar.astro';
---
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>AIO SEO (All-In-One SEO) | Dentistry Growth Partners</title>
  <meta name="description" content="Dominate local search rankings and become the #1 choice for high-value implant cases in your area. Get recommended by ChatGPT and other AI search engines.">
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700;800&display=swap" rel="stylesheet">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@tabler/icons-webfont@latest/tabler-icons.min.css">
    <!-- Favicon -->
  <link rel="icon" type="image/png" href="/636f4050ef41c9124a12cbba/dgp-logo1.png" />
  <link rel="shortcut icon" type="image/png" href="/636f4050ef41c9124a12cbba/dgp-logo1.png" />
  
  <style>
    /* ============================================
       RESET & BASE STYLES
    ============================================ */
    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
    }

    :root {
      --dark-slate-blue: #083981;
      --natural: #c6ff53;
      --white: #ffffff;
      --black: #1d1d1d;
      --grey: #f5f5f5;
      --dim-grey: #646464;
      --dark-olive-green: #194c8b;
      --green: #2b6648;
      --pale-goldenrod: #e3fcb0;
    }

    body {
      font-family: 'Inter', sans-serif;
      background: var(--white);
      color: var(--black);
      line-height: 1.6;
      overflow-x: hidden;
    }

    .section-container {
      max-width: 1400px;
      margin: 0 auto;
      padding: 0 24px;
    }

    /* ============================================
       HERO SECTION
    ============================================ */
    .hero-section {
      min-height: 90vh;
      display: flex;
      align-items: center;
      justify-content: center;
      position: relative;
      overflow: hidden;
      background: linear-gradient(135deg, #1e3a5f 0%, #0a4d9e 50%, #2b6648 100%);
      padding: 140px 24px 80px;
      margin-top: 0;
    }

    /* Animated grid background */
    .hero-section .hero-background {
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      overflow: hidden;
      opacity: 0.15;
      z-index: 0;
    }

    .hero-section .hero-background::before {
      content: '';
      position: absolute;
      width: 100%;
      height: 100%;
      background-image:
        linear-gradient(rgba(198, 255, 83, 0.1) 2px, transparent 2px),
        linear-gradient(90deg, rgba(198, 255, 83, 0.1) 2px, transparent 2px);
      background-size: 60px 60px;
      animation: gridMove 20s linear infinite;
    }

    @keyframes gridMove {
      0% {
        transform: translate(0, 0);
      }
      100% {
        transform: translate(60px, 60px);
      }
    }

    .hero-section .hero-shapes {
      position: absolute;
      width: 100%;
      height: 100%;
      top: 0;
      left: 0;
    }

    .hero-section .shape {
      position: absolute;
      filter: blur(100px);
      opacity: 0.3;
      will-change: transform;
      pointer-events: none;
    }

    .hero-section .shape-1 {
      width: 500px;
      height: 500px;
      background: radial-gradient(circle, rgba(198, 255, 83, 0.4) 0%, transparent 70%);
      top: -150px;
      right: -150px;
      animation: floatSlow 18s infinite ease-in-out;
    }

    .hero-section .shape-2 {
      width: 400px;
      height: 400px;
      background: radial-gradient(circle, rgba(198, 255, 83, 0.3) 0%, transparent 70%);
      bottom: -200px;
      left: -200px;
      animation: floatSlow 22s infinite ease-in-out reverse;
    }

    .hero-section .shape-3 {
      width: 350px;
      height: 350px;
      background: radial-gradient(circle, rgba(255, 255, 255, 0.2) 0%, transparent 70%);
      top: 20%;
      left: 50%;
      transform: translateX(-50%);
      animation: floatSlow 25s infinite ease-in-out;
    }

    @keyframes floatSlow {
      0%, 100% {
        transform: translate(0, 0) scale(1);
      }
      33% {
        transform: translate(40px, -40px) scale(1.1);
      }
      66% {
        transform: translate(-30px, 30px) scale(0.95);
      }
    }

    .hero-section .hero-container {
      max-width: 1200px;
      width: 100%;
      margin: 0 auto;
      position: relative;
      z-index: 10;
      display: flex;
      flex-direction: column;
      align-items: center;
      justify-content: center;
      text-align: center;
    }

    .hero-section .hero-badge {
      display: inline-flex;
      align-items: center;
      gap: 10px;
      background: rgba(255, 255, 255, 0.1);
      backdrop-filter: blur(10px);
      border: 2px solid rgba(198, 255, 83, 0.3);
      padding: 12px 28px;
      border-radius: 50px;
      color: var(--white);
      font-size: 15px;
      font-weight: 700;
      margin-bottom: 32px;
      transition: all 0.3s ease;
    }

    .hero-section .hero-badge i {
      font-size: 20px;
      color: var(--natural);
    }

    .hero-section h1 {
      font-size: clamp(40px, 5.5vw, 68px);
      font-weight: 800;
      color: #ffffff;
      line-height: 1.2;
      margin: 0 auto 24px;
      letter-spacing: -1.8px;
      text-shadow: 0 4px 20px rgba(0, 0, 0, 0.3);
      max-width: 1000px;
      padding: 0 20px;
    }

    .hero-section .highlight-animated {
      color: var(--natural);
      position: relative;
      display: inline-block;
      background: linear-gradient(135deg, #c6ff53 0%, #d4ff8d 100%);
      -webkit-background-clip: text;
      -webkit-text-fill-color: transparent;
      background-clip: text;
    }

    .hero-section .highlight-animated::after {
      content: '';
      position: absolute;
      bottom: -4px;
      left: 0;
      right: 0;
      height: 8px;
      background: #c6ff53;
      opacity: 0.3;
      border-radius: 4px;
      transform: scaleX(0);
      animation: underlineGrow 0.8s ease-out 0.3s forwards;
    }

    @keyframes underlineGrow {
      to {
        transform: scaleX(1);
      }
    }

    .hero-section .hero-subtitle {
      font-size: clamp(18px, 2.2vw, 22px);
      color: rgba(255, 255, 255, 0.95);
      line-height: 1.6;
      margin-bottom: 48px;
      max-width: 900px;
      margin-left: auto;
      margin-right: auto;
    }

    .hero-section .hero-cta {
      display: inline-flex;
      align-items: center;
      gap: 10px;
      padding: 18px 42px;
      background: var(--natural);
      color: var(--dark-slate-blue);
      font-size: 17px;
      font-weight: 700;
      border-radius: 12px;
      text-decoration: none;
      transition: all 0.3s ease;
      border: 3px solid var(--natural);
      box-shadow: 0 8px 24px rgba(198, 255, 83, 0.3);
      margin-bottom: 28px;
    }

    .hero-section .hero-cta:hover {
      background: transparent;
      color: var(--natural);
      transform: translateY(-4px);
      box-shadow: 0 12px 32px rgba(198, 255, 83, 0.4);
    }

    .hero-section .hero-cta i {
      font-size: 22px;
    }

    .hero-section .scroll-indicator {
      display: flex;
      flex-direction: column;
      align-items: center;
      gap: 6px;
      color: rgba(255, 255, 255, 0.7);
      font-size: 13px;
      font-weight: 600;
      cursor: pointer;
      transition: all 0.3s ease;
      margin-top: 8px;
    }

    .hero-section .scroll-indicator:hover {
      color: var(--natural);
      transform: translateY(4px);
    }

    .hero-section .scroll-indicator i {
      font-size: 22px;
      animation: bounce 2s infinite;
    }

    @keyframes bounce {
      0%, 100% {
        transform: translateY(0);
      }
      50% {
        transform: translateY(8px);
      }
    }

    /* ============================================
       FEATURES SECTION
    ============================================ */
    .features-section {
      padding: 120px 24px;
      background: var(--white);
    }

    .section-header {
      max-width: 800px;
      margin: 0 auto 60px;
      text-align: center;
    }

    .section-tag {
      display: inline-flex;
      align-items: center;
      gap: 10px;
      background: var(--pale-goldenrod);
      color: var(--dark-olive-green);
      padding: 10px 24px;
      border-radius: 50px;
      font-size: 14px;
      font-weight: 700;
      margin-bottom: 20px;
    }

    .section-tag i {
      font-size: 18px;
    }

    .section-title {
      font-size: clamp(32px, 4vw, 48px);
      font-weight: 800;
      color: var(--dark-slate-blue);
      margin-bottom: 16px;
      line-height: 1.2;
      letter-spacing: -1px;
    }

    .features-grid {
      display: grid;
      grid-template-columns: repeat(3, 1fr);
      gap: 32px;
      max-width: 1300px;
      margin: 0 auto;
    }

    .feature-card {
      background: var(--white);
      border: 3px solid var(--grey);
      border-radius: 20px;
      padding: 40px 32px;
      transition: all 0.4s ease;
      position: relative;
      overflow: hidden;
    }

    .feature-card::before {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: linear-gradient(135deg, transparent 0%, rgba(198, 255, 83, 0.05) 100%);
      opacity: 0;
      transition: opacity 0.4s ease;
    }

    .feature-card:hover {
      border-color: var(--natural);
      transform: translateY(-8px);
      box-shadow: 0 16px 48px rgba(0, 0, 0, 0.12);
    }

    .feature-card:hover::before {
      opacity: 1;
    }

    .feature-icon {
      width: 70px;
      height: 70px;
      background: linear-gradient(135deg, var(--dark-slate-blue), var(--dark-olive-green));
      border-radius: 16px;
      display: flex;
      align-items: center;
      justify-content: center;
      font-size: 32px;
      color: var(--natural);
      margin-bottom: 24px;
      transition: all 0.4s ease;
    }

    .feature-card:hover .feature-icon {
      transform: scale(1.1) rotate(5deg);
    }

    .feature-card h3 {
      font-size: 22px;
      font-weight: 800;
      color: var(--dark-slate-blue);
      margin-bottom: 12px;
      letter-spacing: -0.3px;
    }

    .feature-card p {
      font-size: 15px;
      color: var(--dim-grey);
      line-height: 1.7;
      margin: 0;
    }

    /* ============================================
       WHY IT MATTERS SECTION
    ============================================ */
    .why-matters-section {
      padding: 120px 24px;
      background: var(--grey);
    }

    .why-matters-intro {
      max-width: 900px;
      margin: 0 auto 60px;
      text-align: center;
    }

    .why-matters-grid {
      display: grid;
      grid-template-columns: repeat(2, 1fr);
      gap: 40px;
      max-width: 1200px;
      margin: 0 auto 60px;
    }

    .benefit-card {
      background: var(--white);
      border: 3px solid transparent;
      border-radius: 24px;
      padding: 48px 40px;
      transition: all 0.4s ease;
      position: relative;
    }

    .benefit-card:hover {
      transform: translateY(-8px);
      box-shadow: 0 20px 60px rgba(0, 0, 0, 0.15);
      border-color: var(--natural);
    }

    .benefit-badge {
      display: inline-flex;
      align-items: center;
      gap: 8px;
      background: linear-gradient(135deg, var(--dark-slate-blue), var(--dark-olive-green));
      color: var(--natural);
      padding: 10px 20px;
      border-radius: 50px;
      font-size: 13px;
      font-weight: 700;
      text-transform: uppercase;
      letter-spacing: 0.5px;
      margin-bottom: 24px;
    }

    .benefit-badge i {
      font-size: 16px;
    }

    .benefit-card h3 {
      font-size: 28px;
      font-weight: 800;
      color: var(--dark-slate-blue);
      margin-bottom: 20px;
      letter-spacing: -0.5px;
    }

    .benefit-card p {
      font-size: 16px;
      color: var(--dim-grey);
      line-height: 1.7;
      margin: 0;
    }

    .stats-highlight {
      max-width: 1100px;
      margin: 0 auto;
      background: linear-gradient(135deg, var(--dark-slate-blue), var(--dark-olive-green));
      border-radius: 24px;
      padding: 48px 40px;
      text-align: center;
      position: relative;
      overflow: hidden;
    }

    .stats-highlight::before {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      background: url('data:image/svg+xml,<svg width="100" height="100" xmlns="http://www.w3.org/2000/svg"><circle cx="50" cy="50" r="2" fill="rgba(198,255,83,0.1)"/></svg>') repeat;
      opacity: 0.3;
    }

    .stats-highlight h3 {
      font-size: 32px;
      font-weight: 800;
      color: var(--white);
      margin-bottom: 32px;
      position: relative;
      z-index: 1;
    }

    .stats-row {
      display: grid;
      grid-template-columns: repeat(2, 1fr);
      gap: 32px;
      position: relative;
      z-index: 1;
    }

    .stat-box {
      background: rgba(255, 255, 255, 0.1);
      backdrop-filter: blur(10px);
      border: 2px solid rgba(198, 255, 83, 0.3);
      border-radius: 16px;
      padding: 32px 24px;
      transition: all 0.3s ease;
    }

    .stat-box:hover {
      background: rgba(255, 255, 255, 0.15);
      border-color: var(--natural);
      transform: translateY(-4px);
    }

    .stat-value {
      font-size: 42px;
      font-weight: 800;
      color: var(--natural);
      margin-bottom: 12px;
      letter-spacing: -1px;
    }

    .stat-label {
      font-size: 16px;
      color: rgba(255, 255, 255, 0.95);
      line-height: 1.5;
    }

    /* ============================================
       PROCESS SECTION
    ============================================ */
    .process-section {
      padding: 120px 24px;
      background: var(--white);
    }

    .process-timeline {
      max-width: 1000px;
      margin: 60px auto 0;
      position: relative;
    }

    .process-timeline::before {
      content: '';
      position: absolute;
      left: 50%;
      top: 0;
      bottom: 0;
      width: 3px;
      background: linear-gradient(180deg, var(--dark-slate-blue), var(--dark-olive-green));
      transform: translateX(-50%);
    }

    .process-step {
      display: grid;
      grid-template-columns: 1fr 80px 1fr;
      gap: 40px;
      margin-bottom: 60px;
      align-items: center;
    }

    .process-step:last-child {
      margin-bottom: 0;
    }

    .step-content-left,
    .step-content-right {
      background: var(--white);
      border: 3px solid var(--grey);
      border-radius: 20px;
      padding: 36px 32px;
      transition: all 0.4s ease;
    }

    .step-content-left:hover,
    .step-content-right:hover {
      border-color: var(--natural);
      transform: translateY(-4px);
      box-shadow: 0 12px 32px rgba(0, 0, 0, 0.1);
    }

    .step-content-left {
      text-align: right;
    }

    .step-content-right {
      text-align: left;
    }

    .step-number {
      width: 80px;
      height: 80px;
      background: linear-gradient(135deg, var(--dark-slate-blue), var(--dark-olive-green));
      border: 4px solid var(--natural);
      border-radius: 50%;
      display: flex;
      align-items: center;
      justify-content: center;
      font-size: 32px;
      font-weight: 800;
      color: var(--natural);
      position: relative;
      z-index: 10;
      box-shadow: 0 8px 24px rgba(0, 0, 0, 0.2);
    }

    .step-content-left h3,
    .step-content-right h3 {
      font-size: 24px;
      font-weight: 800;
      color: var(--dark-slate-blue);
      margin-bottom: 12px;
      letter-spacing: -0.3px;
    }

    .step-content-left p,
    .step-content-right p {
      font-size: 15px;
      color: var(--dim-grey);
      line-height: 1.7;
      margin: 0;
    }

    /* ============================================
       FINAL CTA SECTION
    ============================================ */
    .final-cta-section {
      padding: 100px 24px;
      background: linear-gradient(135deg, var(--dark-slate-blue) 0%, var(--dark-olive-green) 100%);
      position: relative;
      overflow: hidden;
    }

    .final-cta-section::before {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      background: url('data:image/svg+xml,<svg width="100" height="100" xmlns="http://www.w3.org/2000/svg"><circle cx="50" cy="50" r="2" fill="rgba(198,255,83,0.1)"/></svg>') repeat;
      opacity: 0.3;
    }

    .final-cta-content {
      max-width: 900px;
      margin: 0 auto;
      text-align: center;
      position: relative;
      z-index: 10;
    }

    .final-cta-icon {
      width: 100px;
      height: 100px;
      background: rgba(198, 255, 83, 0.15);
      border: 3px solid var(--natural);
      border-radius: 50%;
      display: flex;
      align-items: center;
      justify-content: center;
      margin: 0 auto 32px;
      font-size: 48px;
      color: var(--natural);
    }

    .final-cta-section h2 {
      font-size: clamp(32px, 4vw, 48px);
      font-weight: 800;
      color: var(--white);
      margin-bottom: 24px;
      line-height: 1.2;
      letter-spacing: -1px;
    }

    .final-cta-text {
      font-size: 20px;
      color: rgba(255, 255, 255, 0.95);
      line-height: 1.7;
      margin-bottom: 40px;
      max-width: 800px;
      margin-left: auto;
      margin-right: auto;
    }

    .final-cta-button {
      display: inline-flex;
      align-items: center;
      gap: 12px;
      padding: 20px 48px;
      background: var(--natural);
      color: var(--dark-slate-blue);
      font-size: 18px;
      font-weight: 700;
      border-radius: 14px;
      text-decoration: none;
      transition: all 0.3s ease;
      border: 3px solid var(--natural);
      box-shadow: 0 8px 24px rgba(198, 255, 83, 0.3);
    }

    .final-cta-button:hover {
      background: transparent;
      color: var(--natural);
      transform: translateY(-4px);
      box-shadow: 0 12px 32px rgba(198, 255, 83, 0.4);
    }

    .final-cta-button i {
      font-size: 22px;
    }

    /* ============================================
       FLOATING CTA BUTTONS
    ============================================ */
    .floating-cta {
      position: fixed;
      bottom: 32px;
      right: 32px;
      display: flex;
      flex-direction: column;
      gap: 16px;
      z-index: 1000;
    }

    .floating-btn {
      display: flex;
      align-items: center;
      gap: 12px;
      padding: 16px 24px;
      background: var(--natural);
      color: var(--dark-slate-blue);
      border-radius: 50px;
      text-decoration: none;
      font-weight: 700;
      font-size: 15px;
      box-shadow: 0 8px 24px rgba(0, 0, 0, 0.2);
      transition: all 0.3s ease;
    }

    .floating-btn:hover {
      transform: translateY(-4px);
      box-shadow: 0 12px 32px rgba(0, 0, 0, 0.3);
      background: var(--dark-slate-blue);
      color: var(--natural);
    }

    /* ============================================
       RESPONSIVE DESIGN
    ============================================ */
    @media (max-width: 991px) {
      .hero-section {
        padding: 130px 24px 80px;
        margin-top: 0;
        min-height: 85vh;
      }

      .hero-section .shape-1,
      .hero-section .shape-2,
      .hero-section .shape-3 {
        filter: blur(70px);
      }

      .hero-section .shape-1 {
        width: 400px;
        height: 400px;
      }

      .hero-section .shape-2 {
        width: 350px;
        height: 350px;
      }

      .hero-section .shape-3 {
        width: 300px;
        height: 300px;
      }

      .features-grid {
        grid-template-columns: repeat(2, 1fr);
        gap: 24px;
      }

      .why-matters-grid {
        grid-template-columns: 1fr;
        gap: 24px;
      }

      .stats-row {
        grid-template-columns: 1fr;
        gap: 24px;
      }

      .process-timeline::before {
        display: none;
      }

      .process-step {
        grid-template-columns: 1fr;
        gap: 24px;
        margin-bottom: 40px;
      }

      .step-content-left,
      .step-content-right {
        text-align: left;
      }

      .step-number {
        margin: 0 auto;
      }

      .features-section,
      .why-matters-section,
      .process-section,
      .final-cta-section {
        padding: 80px 24px;
      }
    }

    @media (max-width: 767px) {
      .hero-section {
        padding: 120px 20px 60px;
        margin-top: 0;
        min-height: 80vh;
      }

      .hero-section .hero-background::before {
        background-size: 40px 40px;
      }

      .hero-section h1 {
        font-size: clamp(32px, 7vw, 44px);
        margin-bottom: 20px;
        padding: 0 10px;
      }

      .hero-section .hero-subtitle {
        font-size: 18px;
        margin-bottom: 40px;
      }

      .hero-section .shape-1,
      .hero-section .shape-2,
      .hero-section .shape-3 {
        filter: blur(50px);
      }

      .hero-section .shape-1 {
        width: 300px;
        height: 300px;
        top: -100px;
        right: -100px;
      }

      .hero-section .shape-2 {
        width: 250px;
        height: 250px;
        bottom: -150px;
        left: -150px;
      }

      .hero-section .shape-3 {
        width: 200px;
        height: 200px;
      }

      .hero-section .hero-cta {
        padding: 18px 36px;
        font-size: 16px;
        width: 100%;
        max-width: 400px;
        justify-content: center;
      }

      .hero-section .hero-badge {
        padding: 10px 20px;
        font-size: 13px;
      }

      .features-grid {
        grid-template-columns: 1fr;
        gap: 20px;
      }

      .feature-card {
        padding: 32px 24px;
      }

      .feature-icon {
        width: 60px;
        height: 60px;
        font-size: 28px;
      }

      .benefit-card {
        padding: 36px 28px;
      }

