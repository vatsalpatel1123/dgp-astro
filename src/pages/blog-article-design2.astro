---
// Import components here
import Navbar from '../components/Navbar.astro';
import Footer from '../components/Footer.astro';
import StickyCTA from '../components/StickyCTA.astro';

// This would normally come from your CMS or database based on the slug
const article = {
  id: 1,
  title: "How AI Voice Receptionists Are Revolutionizing Dental Practices in 2025",
  excerpt: "Discover how AI-powered voice technology is helping dental practices answer every call, reduce staff workload, and increase patient bookings by 300%.",
  image: "/images/blog/ai-receptionist-dental.jpg",
  category: "Technology",
  author: {
    name: "Sarah Johnson",
    avatar: "SJ",
    bio: "Marketing Director at Dentistry Growth Partners with 10+ years of experience in dental practice automation and AI implementation.",
    role: "Marketing Director",
    social: {
      twitter: "sarahjohnson",
      linkedin: "sarahjohnson"
    }
  },
  date: "January 15, 2025",
  readTime: "8 min read",
  slug: "ai-voice-receptionists-dental-practices-2025",
  publishedDate: "2025-01-15T09:00:00Z"
};

// Related posts
const relatedPosts = [
  {
    title: "The Complete Guide to Multi-Implant Marketing for Dental Practices",
    image: "/images/blog/multi-implant-marketing.jpg",
    category: "Marketing",
    slug: "complete-guide-multi-implant-marketing",
    date: "January 12, 2025",
    readTime: "12 min read"
  },
  {
    title: "Why Your Dental Practice Is Losing Patients to Voicemail",
    image: "/images/blog/dental-voicemail-problem.jpg",
    category: "Practice Growth",
    slug: "dental-practice-losing-patients-voicemail",
    date: "January 10, 2025",
    readTime: "6 min read"
