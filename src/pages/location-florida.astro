    <style>
      :root {
        --black: #1d1d1d;
        --dim-grey: #646464;
        --dark-olive-green: #194c8b;
        --white: white;
        --dark-slate-blue: #083981;
        --natural: #c6ff53;
        --grey: whitesmoke;
        --green: #2b6648;
        --orange: #eb742e;
        --dark-grey: #9f9f9f;
        --misty-rose: #ffe4d4;
        --pale-goldenrod: #e3fcb0;
        --indian-red: #db5656;
        --blue: #474bc5;
        --silver: #c5c5c5;
        --honeydew: #e7fff3;
      }

      * {
        margin: 0;
        padding: 0;
        box-sizing: border-box;
      }

      body {
        font-family: 'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif;
        color: var(--black);
        line-height: 1.6;
        overflow-x: hidden;
      }

      /* Hero Section */
      .hero-section {
        background: linear-gradient(135deg, var(--dark-slate-blue) 0%, var(--dark-olive-green) 100%);
        padding: 140px 24px 100px;
        position: relative;
        overflow: hidden;
      }

      .hero-section::before {
        content: '';
        position: absolute;
        top: 0;
        right: 0;
        width: 50%;
        height: 100%;
        background: url('data:image/svg+xml,<svg width="100" height="100" xmlns="http://www.w3.org/2000/svg"><circle cx="50" cy="50" r="2" fill="rgba(198,255,83,0.1)"/></svg>') repeat;
        opacity: 0.3;
      }

      .hero-container {
        max-width: 1280px;
        margin: 0 auto;
        display: grid;
        grid-template-columns: 1.2fr 1fr;
        gap: 80px;
        align-items: center;
        position: relative;
        z-index: 1;
      }

      .hero-content h1 {
        font-size: 58px;
        font-weight: 800;
        color: var(--white);
        line-height: 1.1;
        margin-bottom: 24px;
        letter-spacing: -1.5px;
      }

      .hero-content .highlight {
        color: var(--natural);
      }

      .hero-content .subtitle {
        font-size: 22px;
        color: rgba(255,255,255,0.95);
        margin-bottom: 16px;
        font-weight: 600;
        line-height: 1.4;
      }

      .hero-content p {
        font-size: 18px;
        color: rgba(255,255,255,0.85);
        margin-bottom: 20px;
        line-height: 1.7;
      }

      .location-badge {
        display: inline-flex;
        align-items: center;
        gap: 10px;
        background: rgba(198,255,83,0.2);
        color: var(--natural);
        padding: 12px 24px;
        border-radius: 24px;
        font-size: 15px;
        font-weight: 700;
        margin-bottom: 24px;
        border: 2px solid rgba(198,255,83,0.3);
      }

      .cities-served {
        background: rgba(255,255,255,0.1);
        padding: 24px;
        border-radius: 16px;
        margin-top: 32px;
        backdrop-filter: blur(10px);
        border: 2px solid rgba(198,255,83,0.3);
      }

      .cities-served h3 {
        font-size: 16px;
        color: var(--natural);
        margin-bottom: 16px;
        font-weight: 700;
      }

      .cities-list {
        display: flex;
        flex-wrap: wrap;
        gap: 12px;
      }

      .city-tag {
        background: rgba(255,255,255,0.15);
        color: var(--white);
        padding: 8px 16px;
        border-radius: 20px;
        font-size: 14px;
        font-weight: 600;
        border: 1px solid rgba(255,255,255,0.2);
      }

      .hero-stats {
        background: var(--white);
        border-radius: 24px;
        padding: 40px;
        box-shadow: 0 30px 80px rgba(0,0,0,0.25);
        border: 4px solid var(--natural);
        animation: float 6s ease-in-out infinite;
      }

      @keyframes float {
        0%, 100% { transform: translateY(0) rotate(0deg); }
        50% { transform: translateY(-20px) rotate(1deg); }
      }

      .stat-grid {
        display: grid;
        grid-template-columns: repeat(2, 1fr);
        gap: 24px;
      }

      .stat-item {
        text-align: center;
      }

      .stat-number {
        font-size: 42px;
        font-weight: 800;
        color: var(--dark-slate-blue);
        display: block;
        margin-bottom: 8px;
        line-height: 1;
      }

      .stat-label {
        font-size: 13px;
        color: var(--dim-grey);
        font-weight: 600;
      }

      /* CTA Buttons */
      .cta-buttons {
        display: flex;
        gap: 16px;
        flex-wrap: wrap;
        margin-top: 30px;
      }

      .btn-primary {
        background: var(--natural);
        color: var(--black);
        padding: 18px 40px;
        border-radius: 12px;
        text-decoration: none;
        font-weight: 700;
        font-size: 17px;
        transition: all 0.3s ease;
        display: inline-flex;
        align-items: center;
        gap: 10px;
        border: 2px solid var(--natural);
        box-shadow: 0 4px 16px rgba(198,255,83,0.3);
      }

      .btn-primary:hover {
        background: transparent;
        color: var(--natural);
        transform: translateY(-3px);
        box-shadow: 0 6px 24px rgba(198,255,83,0.4);
      }

      .btn-secondary {
        background: transparent;
        color: var(--white);
        padding: 18px 40px;
        border-radius: 12px;
        text-decoration: none;
        font-weight: 700;
        font-size: 17px;
        transition: all 0.3s ease;
        border: 2px solid var(--white);
        display: inline-flex;
        align-items: center;
        gap: 10px;
      }

      .btn-secondary:hover {
        background: var(--white);
        color: var(--dark-slate-blue);
        transform: translateY(-3px);
      }

      /* Section Styles */
      .section-container {
        max-width: 1280px;
        margin: 0 auto;
        padding: 0 24px;
      }

      .section-header {
        text-align: center;
        max-width: 850px;
        margin: 0 auto 60px;
      }

      .section-tag {
        display: inline-block;
        background: var(--pale-goldenrod);
        color: var(--green);
        padding: 10px 24px;
        border-radius: 24px;
        font-size: 13px;
        font-weight: 700;
        margin-bottom: 20px;
        letter-spacing: 1px;
        text-transform: uppercase;
      }

      .section-title {
        font-size: 46px;
        font-weight: 800;
        color: var(--black);
        margin-bottom: 20px;
        line-height: 1.2;
      }

      .section-description {
        font-size: 19px;
        color: var(--dim-grey);
        line-height: 1.7;
      }

      /* Services Section */
      .services-section {
        padding: 100px 24px;
        background: var(--white);
      }

      .services-grid {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(320px, 1fr));
        gap: 32px;
        margin-top: 60px;
      }

      .service-card {
        background: var(--grey);
        border-radius: 20px;
        padding: 40px;
        border: 3px solid var(--natural);
        transition: all 0.3s ease;
      }

      .service-card:hover {
        transform: translateY(-10px);
        box-shadow: 0 16px 48px rgba(43, 102, 72, 0.2);
        background: var(--white);
      }

      .service-icon {
        width: 70px;
        height: 70px;
        background: linear-gradient(135deg, var(--dark-slate-blue), var(--dark-olive-green));
        border-radius: 16px;
        display: flex;
        align-items: center;
        justify-content: center;
        margin-bottom: 24px;
        box-shadow: 0 8px 24px rgba(8, 57, 129, 0.25);
      }

      .service-card h3 {
        font-size: 24px;
        font-weight: 700;
        color: var(--black);
        margin-bottom: 14px;
      }

      .service-card p {
        color: var(--dim-grey);
        line-height: 1.7;
        font-size: 15px;
        margin-bottom: 20px;
      }

      .service-link {
        color: var(--dark-slate-blue);
        font-weight: 600;
        text-decoration: none;
        display: inline-flex;
        align-items: center;
        gap: 8px;
        transition: color 0.3s ease;
      }

      .service-link:hover {
        color: var(--green);
      }

      /* Testimonials Section */
      .testimonials-section {
        padding: 100px 24px;
        background: linear-gradient(to bottom, var(--honeydew), var(--white));
      }

      .testimonials-grid {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(350px, 1fr));
        gap: 32px;
        margin-top: 60px;
      }

      .testimonial-card {
        background: var(--white);
        border-radius: 20px;
        padding: 40px;
        border: 3px solid var(--natural);
        position: relative;
        transition: all 0.3s ease;
      }

      .testimonial-card:hover {
        transform: translateY(-10px);
        box-shadow: 0 16px 48px rgba(43, 102, 72, 0.2);
      }

      .quote-icon {
        width: 60px;
        height: 60px;
        background: linear-gradient(135deg, var(--dark-slate-blue), var(--dark-olive-green));
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        margin-bottom: 24px;
        box-shadow: 0 6px 20px rgba(8, 57, 129, 0.25);
      }

      .testimonial-text {
        font-size: 16px;
        line-height: 1.8;
        color: var(--black);
        margin-bottom: 28px;
        font-style: italic;
      }

      .testimonial-author {
        display: flex;
        align-items: center;
        gap: 16px;
        padding-top: 24px;
        border-top: 2px solid var(--grey);
      }

      .author-avatar {
        width: 60px;
        height: 60px;
        background: linear-gradient(135deg, var(--green), var(--dark-olive-green));
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        font-size: 24px;
        font-weight: 800;
        color: var(--white);
        flex-shrink: 0;
      }

      .author-info h5 {
        font-size: 17px;
        font-weight: 700;
        color: var(--black);
        margin-bottom: 4px;
      }

      .author-info p {
        font-size: 14px;
        color: var(--dim-grey);
        margin: 0;
      }

      .rating {
        color: var(--orange);
        font-size: 16px;
        margin-top: 6px;
      }

      /* FAQ Section */
      .faq-section {
        padding: 100px 24px;
        background: var(--white);
      }

      .faq-container {
        max-width: 900px;
        margin: 60px auto 0;
      }

      .faq-item {
        background: var(--grey);
        border-radius: 16px;
        margin-bottom: 16px;
        border: 2px solid transparent;
        transition: all 0.3s ease;
        overflow: hidden;
      }

      .faq-question {
        padding: 28px 32px;
        cursor: pointer;
        display: flex;
        justify-content: space-between;
        align-items: center;
        gap: 20px;
        font-size: 19px;
        font-weight: 700;
        color: var(--black);
        user-select: none;
      }

      .faq-item:hover {
        border-color: var(--natural);
      }

      .faq-icon {
        width: 32px;
        height: 32px;
        background: var(--natural);
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        font-size: 20px;
        font-weight: 800;
        color: var(--black);
        flex-shrink: 0;
        transition: transform 0.3s ease;
      }

      .faq-item.active .faq-icon {
        transform: rotate(45deg);
      }

      .faq-answer {
        max-height: 0;
        overflow: hidden;
        transition: max-height 0.3s ease;
      }

      .faq-answer-content {
        padding: 0 32px 28px;
        font-size: 16px;
        color: var(--dim-grey);
        line-height: 1.8;
      }

      .faq-item.active .faq-answer {
        max-height: 500px;
      }

      /* Form Section */
      .form-section {
        padding: 100px 24px;
        background: var(--dark-slate-blue);
      }

      .form-section .section-tag {
        background: rgba(198,255,83,0.2);
        color: var(--natural);
      }

      .form-section .section-title,
      .form-section .section-description {
        color: var(--white);
      }

      .form-container {
        max-width: 800px;
        margin: 60px auto 0;
        background: rgba(255,255,255,0.05);
        border-radius: 24px;
        padding: 50px;
        border: 2px solid rgba(198,255,83,0.3);
        backdrop-filter: blur(10px);
      }

      .contact-form {
        display: flex;
        flex-direction: column;
        gap: 24px;
      }

      .form-row {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
        gap: 24px;
      }

      .form-group {
        display: flex;
        flex-direction: column;
        gap: 8px;
      }

      .form-group label {
        font-size: 15px;
        font-weight: 600;
        color: var(--white);
      }

      .form-group input,
      .form-group textarea,
      .form-group select {
        padding: 16px;
        border: 2px solid rgba(255,255,255,0.2);
        border-radius: 12px;
        font-size: 16px;
        font-family: inherit;
        transition: all 0.3s ease;
        background: rgba(255,255,255,0.1);
        color: var(--white);
      }

      .form-group input::placeholder,
      .form-group textarea::placeholder {
        color: rgba(255,255,255,0.5);
      }

      .form-group input:focus,
      .form-group textarea:focus,
      .form-group select:focus {
        outline: none;
        border-color: var(--natural);
        background: rgba(255,255,255,0.15);
        box-shadow: 0 0 0 3px rgba(198,255,83,0.2);
      }

      .form-group select {
        cursor: pointer;
      }

      .form-group textarea {
        min-height: 150px;
        resize: vertical;
      }

      .form-required {
        color: var(--natural);
        margin-left: 4px;
      }

      .submit-btn {
        background: var(--natural);
        color: var(--black);
        padding: 18px 40px;
        border-radius: 12px;
        font-weight: 700;
        font-size: 17px;
        transition: all 0.3s ease;
        display: inline-flex;
        align-items: center;
        justify-content: center;
        gap: 10px;
        border: 2px solid var(--natural);
        box-shadow: 0 4px 16px rgba(198,255,83,0.3);
        cursor: pointer;
      }

      .submit-btn:hover:not(:disabled) {
        background: var(--green);
        border-color: var(--green);
        color: var(--white);
        transform: translateY(-3px);
        box-shadow: 0 6px 24px rgba(43, 102, 72, 0.4);
      }

      .submit-btn:disabled {
        opacity: 0.6;
        cursor: not-allowed;
      }

      .form-message {
        padding: 16px;
        border-radius: 12px;
        font-size: 15px;
        font-weight: 600;
        text-align: center;
        display: none;
      }

      .form-message.success {
        background: var(--pale-goldenrod);
        color: var(--green);
        display: block;
      }

      .form-message.error {
        background: var(--misty-rose);
        color: var(--indian-red);
        display: block;
      }

      /* Cities Section */
      .cities-section {
        padding: 100px 24px;
        background: linear-gradient(to bottom, var(--honeydew), var(--white));
      }

      .cities-grid {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
        gap: 24px;
        margin-top: 60px;
      }

      .city-card {
        background: var(--white);
        border-radius: 16px;
        padding: 32px;
        border: 2px solid var(--natural);
        text-align: center;
        transition: all 0.3s ease;
      }

      .city-card:hover {
        transform: translateY(-8px);
        box-shadow: 0 12px 36px rgba(43, 102, 72, 0.15);
      }

      .city-card h3 {
        font-size: 22px;
        font-weight: 700;
        color: var(--black);
        margin-bottom: 8px;
      }

      .city-card p {
        font-size: 14px;
        color: var(--dim-grey);
      }

      /* Responsive Design */
      @media (max-width: 991px) {
        .hero-container {
          grid-template-columns: 1fr;
          gap: 50px;
        }

        .hero-content h1 {
          font-size: 44px;
        }

        .stat-grid {
          grid-template-columns: repeat(2, 1fr);
        }
      }

      @media (max-width: 767px) {
        .hero-content h1 {
          font-size: 38px;
        }

        .hero-content .subtitle {
          font-size: 19px;
        }

        .section-title {
          font-size: 36px;
        }

        .cta-buttons {
          flex-direction: column;
        }

        .btn-primary,
        .btn-secondary {
          width: 100%;
          justify-content: center;
        }

        .form-container {
          padding: 30px;
        }

        .form-row {
          grid-template-columns: 1fr;
        }

        .testimonials-grid {
          grid-template-columns: 1fr;
        }
      }

      @media (max-width: 479px) {
        .hero-content h1 {
          font-size: 32px;
        }

        .section-title {
          font-size: 30px;
        }

        .stat-grid {
          grid-template-columns: 1fr;
        }
      }

      /* Sticky CTA Bar */
      .sticky-cta {
        position: fixed;
        bottom: 0;
        left: 0;
        right: 0;
        background: var(--black);
        padding: 18px 24px;
        box-shadow: 0 -6px 24px rgba(0,0,0,0.25);
        z-index: 999;
        display: none;
        border-top: 3px solid var(--natural);
      }

      .sticky-cta.active {
        display: block;
      }

      .sticky-cta-content {
        max-width: 1280px;
        margin: 0 auto;
        display: flex;
        justify-content: space-between;
        align-items: center;
        gap: 20px;
      }

      .sticky-cta-text {
        color: var(--white);
        font-weight: 700;
        font-size: 17px;
      }

      @media (max-width: 767px) {
        .sticky-cta-content {
          flex-direction: column;
          text-align: center;
        }
      }
    </style>
