---
// Import components here
import Navbar from '../components/Navbar.astro';
import Footer from '../components/Footer.astro';
import StickyCTA from '../components/StickyCTA.astro';
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>High-Value Case Marketing + Booking System | Dentistry Growth Partners</title>
    <meta
      name="description"
      content="Complete patient acquisition system for high-value dental cases. AI-powered marketing, instant engagement, and automated scheduling for cosmetic, full-mouth reconstruction, and premium treatments."
    />
        <!-- Tabler Icons CDN -->
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@tabler/icons-webfont@latest/tabler-icons.min.css">
    <style>
      :root {
        --black: #1d1d1d;
        --dim-grey: #646464;
        --dark-olive-green: #194c8b;
        --white: white;
        --dark-slate-blue: #083981;
        --natural: #c6ff53;
        --grey: whitesmoke;
        --green: #2b6648;
        --orange: #eb742e;
        --dark-grey: #9f9f9f;
        --misty-rose: #ffe4d4;
        --pale-goldenrod: #e3fcb0;
        --indian-red: #db5656;
        --blue: #474bc5;
        --silver: #c5c5c5;
        --honeydew: #e7fff3;
      }

      * {
        margin: 0;
        padding: 0;
        box-sizing: border-box;
      }

      body {
        font-family: 'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif;
        color: var(--black);
        line-height: 1.6;
        overflow-x: hidden;
      }

      /* Hero Section */
      .hero-section {
        background: linear-gradient(135deg, var(--dark-slate-blue) 0%, var(--dark-olive-green) 100%);
        padding: 140px 24px 100px;
        position: relative;
        overflow: hidden;
      }

      .hero-section::before {
        content: '';
        position: absolute;
        top: 0;
        right: 0;
        width: 50%;
        height: 100%;
        background: url('data:image/svg+xml,<svg width="100" height="100" xmlns="http://www.w3.org/2000/svg"><circle cx="50" cy="50" r="2" fill="rgba(198,255,83,0.1)"/></svg>') repeat;
        opacity: 0.3;
      }

      .hero-section::after {
        content: '';
        position: absolute;
        bottom: -100px;
        left: -100px;
        width: 400px;
        height: 400px;
        background: radial-gradient(circle, rgba(198,255,83,0.15) 0%, transparent 70%);
        border-radius: 50%;
      }

      .hero-container {
        max-width: 1280px;
        margin: 0 auto;
        display: grid;
        grid-template-columns: 1.2fr 1fr;
        gap: 80px;
        align-items: center;
        position: relative;
        z-index: 1;
      }

      .hero-content h1 {
        font-size: 58px;
        font-weight: 800;
        color: var(--white);
        line-height: 1.1;
        margin-bottom: 24px;
        letter-spacing: -1.5px;
      }

      .hero-content .highlight {
        color: var(--natural);
        position: relative;
        display: inline-block;
      }

      .hero-content .subtitle {
        font-size: 22px;
        color: rgba(255,255,255,0.9);
        margin-bottom: 16px;
        font-weight: 500;
        line-height: 1.5;
      }

      .hero-content p {
        font-size: 18px;
        color: rgba(255,255,255,0.85);
        margin-bottom: 36px;
        line-height: 1.7;
      }

      .hero-stats {
        display: grid;
        grid-template-columns: repeat(3, 1fr);
        gap: 32px;
        margin-top: 48px;
        padding-top: 32px;
        border-top: 1px solid rgba(255,255,255,0.2);
      }

      .stat-item {
        text-align: center;
      }

      .stat-number {
        font-size: 48px;
        font-weight: 800;
        color: var(--natural);
        display: block;
        line-height: 1;
        margin-bottom: 8px;
      }

      .stat-label {
        font-size: 14px;
        color: rgba(255,255,255,0.8);
        display: block;
        font-weight: 500;
      }

      .hero-visual {
        position: relative;
        padding: 40px;
      }

      .visual-card {
        background: var(--white);
        border-radius: 20px;
        padding: 40px;
        box-shadow: 0 30px 80px rgba(0,0,0,0.25);
        position: relative;
        animation: float 6s ease-in-out infinite;
        border: 3px solid var(--natural);
      }

      @keyframes float {
        0%, 100% { transform: translateY(0) rotate(0deg); }
        50% { transform: translateY(-20px) rotate(1deg); }
      }

      .visual-icon {
        width: 90px;
        height: 90px;
        background: linear-gradient(135deg, var(--dark-slate-blue), var(--dark-olive-green));
        border-radius: 18px;
        display: flex;
        align-items: center;
        justify-content: center;
        margin-bottom: 24px;
        font-size: 42px;
        box-shadow: 0 8px 24px rgba(8, 57, 129, 0.3);
      }

      .visual-title {
        font-size: 26px;
        font-weight: 700;
        color: var(--black);
        margin-bottom: 12px;
      }

      .visual-text {
        color: var(--dim-grey);
        font-size: 15px;
        line-height: 1.6;
      }

      .visual-badge {
        position: absolute;
        top: -12px;
        right: -12px;
        background: var(--orange);
        color: var(--white);
        padding: 8px 16px;
        border-radius: 20px;
        font-size: 12px;
        font-weight: 700;
        text-transform: uppercase;
        letter-spacing: 0.5px;
        box-shadow: 0 4px 12px rgba(235, 116, 46, 0.4);
      }

      /* CTA Buttons */
      .cta-buttons {
        display: flex;
        gap: 16px;
        flex-wrap: wrap;
      }

      .btn-primary {
        background: var(--natural);
        color: var(--black);
        padding: 18px 40px;
        border-radius: 12px;
        text-decoration: none;
        font-weight: 700;
        font-size: 17px;
        transition: all 0.3s ease;
        display: inline-flex;
        align-items: center;
        gap: 10px;
        border: 2px solid var(--natural);
        box-shadow: 0 4px 16px rgba(198,255,83,0.3);
      }

      .btn-primary:hover {
        background: transparent;
        color: var(--natural);
        transform: translateY(-3px);
        box-shadow: 0 6px 24px rgba(198,255,83,0.4);
      }

      .btn-secondary {
        background: transparent;
        color: var(--white);
        padding: 18px 40px;
        border-radius: 12px;
        text-decoration: none;
        font-weight: 700;
        font-size: 17px;
        transition: all 0.3s ease;
        border: 2px solid var(--white);
        display: inline-flex;
        align-items: center;
        gap: 10px;
      }

      .btn-secondary:hover {
        background: var(--white);
        color: var(--dark-slate-blue);
        transform: translateY(-3px);
      }

      /* Challenge Section */
      .challenge-section {
        padding: 100px 24px;
        background: var(--white);
      }

      .section-container {
        max-width: 1280px;
        margin: 0 auto;
      }

      .section-header {
        text-align: center;
        max-width: 850px;
        margin: 0 auto 60px;
      }

      .section-tag {
        display: inline-block;
        background: var(--pale-goldenrod);
        color: var(--green);
        padding: 10px 24px;
        border-radius: 24px;
        font-size: 13px;
        font-weight: 700;
        margin-bottom: 20px;
        letter-spacing: 1px;
        text-transform: uppercase;
      }

      .section-title {
        font-size: 46px;
        font-weight: 800;
        color: var(--black);
        margin-bottom: 20px;
        line-height: 1.2;
      }

      .section-description {
        font-size: 19px;
        color: var(--dim-grey);
        line-height: 1.7;
      }

      .challenge-grid {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(320px, 1fr));
        gap: 28px;
        margin-top: 50px;
      }

      .challenge-card {
        background: var(--grey);
        border-radius: 20px;
        padding: 36px;
        transition: all 0.4s ease;
        border: 3px solid transparent;
        position: relative;
        overflow: hidden;
      }

      .challenge-card::before {
        content: '';
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 4px;
        background: linear-gradient(90deg, var(--dark-slate-blue), var(--dark-olive-green));
        transform: scaleX(0);
        transition: transform 0.4s ease;
      }

      .challenge-card:hover::before {
        transform: scaleX(1);
      }

      .challenge-card:hover {
        transform: translateY(-10px);
        border-color: var(--dark-olive-green);
        box-shadow: 0 16px 48px rgba(25, 76, 139, 0.2);
        background: var(--white);
      }

      .challenge-icon {
        width: 70px;
        height: 70px;
        background: linear-gradient(135deg, var(--dark-slate-blue), var(--dark-olive-green));
        border-radius: 16px;
        display: flex;
        align-items: center;
        justify-content: center;
        margin-bottom: 24px;
        font-size: 32px;
        box-shadow: 0 8px 20px rgba(8, 57, 129, 0.3);
      }

      .challenge-card h3 {
        font-size: 23px;
        font-weight: 700;
        color: var(--black);
        margin-bottom: 14px;
        line-height: 1.3;
      }

      .challenge-card p {
        color: var(--dim-grey);
        line-height: 1.7;
        font-size: 15px;
      }

      /* Solution Section */
      .solution-section {
        padding: 100px 24px;
        background: linear-gradient(to bottom, var(--honeydew), var(--white));
      }

      .solution-intro {
        max-width: 900px;
        margin: 0 auto 60px;
        text-align: center;
      }

      .solution-intro h2 {
        font-size: 44px;
        font-weight: 800;
        color: var(--black);
        margin-bottom: 20px;
      }

      .solution-intro p {
        font-size: 18px;
        color: var(--dim-grey);
        line-height: 1.7;
      }

      .solution-pipeline {
        background: var(--white);
        border-radius: 28px;
        padding: 60px;
        box-shadow: 0 24px 64px rgba(0,0,0,0.08);
        margin-top: 50px;
        border: 2px solid var(--natural);
      }

      .pipeline-steps {
        display: flex;
        flex-direction: column;
        gap: 48px;
      }

      .pipeline-step {
        display: grid;
        grid-template-columns: 90px 1fr;
        gap: 36px;
        align-items: start;
        position: relative;
      }

      .pipeline-step:not(:last-child)::after {
        content: '';
        position: absolute;
        left: 45px;
        top: 90px;
        bottom: -48px;
        width: 3px;
        background: linear-gradient(to bottom, var(--dark-slate-blue), var(--natural));
      }

      .step-number {
        width: 90px;
        height: 90px;
        background: linear-gradient(135deg, var(--dark-slate-blue), var(--dark-olive-green));
        color: var(--white);
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        font-size: 36px;
        font-weight: 800;
        box-shadow: 0 10px 28px rgba(8, 57, 129, 0.3);
        border: 4px solid var(--natural);
        position: relative;
        z-index: 1;
      }

      .step-content h3 {
        font-size: 30px;
        font-weight: 700;
        color: var(--black);
        margin-bottom: 14px;
      }

      .step-label {
        display: inline-block;
        background: var(--pale-goldenrod);
        color: var(--green);
        padding: 6px 14px;
        border-radius: 14px;
        font-size: 13px;
        font-weight: 700;
        margin-bottom: 18px;
        text-transform: uppercase;
        letter-spacing: 0.5px;
      }

      .step-content p {
        color: var(--dim-grey);
        line-height: 1.8;
        font-size: 16px;
        margin-bottom: 20px;
      }

      .step-features {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(220px, 1fr));
        gap: 14px;
        margin-top: 24px;
        background: var(--grey);
        padding: 24px;
        border-radius: 16px;
      }

      .feature-item {
        display: flex;
        align-items: center;
        gap: 10px;
        color: var(--black);
        font-size: 14px;
        font-weight: 500;
      }

      .feature-item::before {
        content: '✓';
        color: var(--green);
        font-weight: bold;
        font-size: 18px;
        flex-shrink: 0;
      }

      /* Timeline Section */
      .timeline-section {
        padding: 100px 24px;
        background: var(--black);
      }

      .timeline-section .section-tag {
        background: rgba(198,255,83,0.2);
        color: var(--natural);
      }

      .timeline-section .section-title,
      .timeline-section .section-description {
        color: var(--white);
      }

      .timeline-container {
        max-width: 1100px;
        margin: 60px auto 0;
        position: relative;
      }

      .timeline-line {
        position: absolute;
        left: 45px;
        top: 0;
        bottom: 0;
        width: 4px;
        background: linear-gradient(to bottom, var(--natural), var(--green));
        border-radius: 4px;
      }

      .timeline-item {
        display: grid;
        grid-template-columns: 90px 1fr;
        gap: 36px;
        margin-bottom: 60px;
        position: relative;
      }

      .timeline-day {
        width: 90px;
        height: 90px;
        background: var(--dark-slate-blue);
        color: var(--white);
        border-radius: 50%;
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        font-weight: 700;
        position: relative;
        z-index: 1;
        box-shadow: 0 8px 24px rgba(198,255,83,0.3);
        border: 4px solid var(--natural);
      }

      .day-label {
        font-size: 11px;
        text-transform: uppercase;
        letter-spacing: 0.5px;
        color: var(--natural);
      }

      .day-number {
        font-size: 26px;
        color: var(--white);
      }

      .timeline-content {
        background: rgba(255,255,255,0.05);
        border: 2px solid rgba(198,255,83,0.2);
        border-radius: 20px;
        padding: 32px;
        backdrop-filter: blur(10px);
        transition: all 0.3s ease;
      }

      .timeline-content:hover {
        background: rgba(255,255,255,0.08);
        border-color: var(--natural);
        transform: translateX(8px);
      }

      .timeline-content h3 {
        font-size: 24px;
        font-weight: 700;
        color: var(--white);
        margin-bottom: 14px;
      }

      .timeline-content p {
        color: rgba(255,255,255,0.85);
        line-height: 1.8;
        font-size: 15px;
      }

      /* Features Section */
      .features-section {
        padding: 100px 24px;
        background: var(--white);
      }

      .features-grid {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
        gap: 32px;
        margin-top: 60px;
      }

      .feature-card {
        background: var(--grey);
        border-radius: 20px;
        padding: 40px;
        transition: all 0.4s ease;
        border: 3px solid transparent;
        text-align: center;
        position: relative;
        overflow: hidden;
      }

      .feature-card::before {
        content: '';
        position: absolute;
        top: 0;
        left: -100%;
        width: 100%;
        height: 100%;
        background: linear-gradient(90deg, transparent, rgba(198,255,83,0.1), transparent);
        transition: left 0.5s ease;
      }

      .feature-card:hover::before {
        left: 100%;
      }

      .feature-card:hover {
        transform: translateY(-10px);
        border-color: var(--natural);
        box-shadow: 0 16px 48px rgba(43, 102, 72, 0.15);
        background: var(--white);
      }

      .feature-card-icon {
        width: 80px;
        height: 80px;
        background: linear-gradient(135deg, var(--green), var(--dark-olive-green));
        border-radius: 20px;
        display: flex;
        align-items: center;
        justify-content: center;
        margin: 0 auto 24px;
        font-size: 36px;
        box-shadow: 0 8px 24px rgba(43, 102, 72, 0.25);
      }

      .feature-card h3 {
        font-size: 21px;
        font-weight: 700;
        color: var(--black);
        margin-bottom: 14px;
      }

      .feature-card p {
        color: var(--dim-grey);
        line-height: 1.7;
        font-size: 15px;
      }

      /* Case Types Section */
      .case-types-section {
        padding: 100px 24px;
        background: linear-gradient(135deg, var(--dark-slate-blue), var(--black));
      }

      .case-types-section .section-tag {
        background: rgba(198,255,83,0.2);
        color: var(--natural);
      }

      .case-types-section .section-title,
      .case-types-section .section-description {
        color: var(--white);
      }

      .case-types-grid {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
        gap: 28px;
        margin-top: 60px;
      }

      .case-type-card {
        background: rgba(255,255,255,0.05);
        border: 2px solid rgba(198,255,83,0.3);
        border-radius: 20px;
        padding: 36px;
        backdrop-filter: blur(10px);
        transition: all 0.4s ease;
        position: relative;
        overflow: hidden;
      }

      .case-type-card::before {
        content: '';
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        height: 4px;
        background: linear-gradient(90deg, var(--natural), var(--green));
        transform: scaleX(0);
        transition: transform 0.4s ease;
      }

      .case-type-card:hover::before {
        transform: scaleX(1);
      }

      .case-type-card:hover {
        background: rgba(255,255,255,0.1);
        border-color: var(--natural);
        transform: translateY(-8px);
      }

      .case-type-icon {
        font-size: 48px;
        margin-bottom: 20px;
        display: block;
      }

      .case-type-card h3 {
        font-size: 24px;
        font-weight: 700;
        color: var(--white);
        margin-bottom: 14px;
      }

      .case-type-card p {
        color: rgba(255,255,255,0.85);
        line-height: 1.7;
        font-size: 15px;
        margin-bottom: 20px;
      }

      .case-value {
        display: inline-block;
        background: var(--natural);
        color: var(--black);
        padding: 6px 14px;
        border-radius: 12px;
        font-size: 13px;
        font-weight: 700;
        margin-top: 8px;
      }

      /* Benefits Section */
      .benefits-section {
        padding: 100px 24px;
        background: var(--misty-rose);
      }

      .benefits-grid {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
        gap: 32px;
        margin-top: 60px;
      }

      .benefit-card {
        background: var(--white);
        border: 2px solid var(--orange);
        border-radius: 20px;
        padding: 36px;
        transition: all 0.3s ease;
        box-shadow: 0 4px 16px rgba(235, 116, 46, 0.1);
      }

      .benefit-card:hover {
        transform: translateY(-10px);
        box-shadow: 0 16px 40px rgba(235, 116, 46, 0.2);
      }

      .benefit-icon {
        font-size: 44px;
        margin-bottom: 20px;
        display: block;
      }

      .benefit-card h3 {
        font-size: 23px;
        font-weight: 700;
        color: var(--black);
        margin-bottom: 14px;
      }

      .benefit-card p {
        color: var(--dim-grey);
        line-height: 1.7;
        font-size: 15px;
      }

      /* ROI Section */
      .roi-section {
        padding: 100px 24px;
        background: var(--white);
      }

      .roi-calculator {
        max-width: 900px;
        margin: 60px auto 0;
        background: var(--grey);
        border-radius: 28px;
        padding: 50px;
        border: 3px solid var(--dark-slate-blue);
        box-shadow: 0 20px 60px rgba(0,0,0,0.1);
      }

      .roi-stats {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
        gap: 32px;
        margin-top: 40px;
      }

      .roi-stat {
        text-align: center;
        padding: 24px;
        background: var(--white);
        border-radius: 16px;
        border: 2px solid var(--natural);
      }

      .roi-stat-number {
        font-size: 42px;
        font-weight: 800;
        color: var(--green);
        display: block;
        margin-bottom: 8px;
      }

      .roi-stat-label {
        font-size: 14px;
        color: var(--dim-grey);
        font-weight: 600;
      }

      .roi-calculation {
        margin-top: 40px;
        padding: 32px;
        background: var(--white);
        border-radius: 20px;
        border-left: 4px solid var(--orange);
      }

      .roi-calculation h4 {
        font-size: 20px;
        font-weight: 700;
        color: var(--black);
        margin-bottom: 16px;
      }

      .roi-calculation p {
        color: var(--dim-grey);
        line-height: 1.8;
        font-size: 16px;
      }

      /* Pricing Section */
      .pricing-section {
        padding: 100px 24px;
        background: linear-gradient(to bottom, var(--honeydew), var(--white));
      }

      .pricing-card {
        max-width: 650px;
        margin: 60px auto 0;
        background: var(--white);
        border-radius: 28px;
        padding: 50px;
        border: 4px solid var(--natural);
        position: relative;
        overflow: hidden;
        box-shadow: 0 24px 64px rgba(43, 102, 72, 0.15);
      }

      .pricing-badge {
        position: absolute;
        top: 28px;
        right: 28px;
        background: var(--orange);
        color: var(--white);
        padding: 10px 20px;
        border-radius: 24px;
        font-size: 13px;
        font-weight: 700;
        text-transform: uppercase;
        letter-spacing: 0.5px;
        box-shadow: 0 4px 12px rgba(235, 116, 46, 0.4);
      }

      .pricing-header {
        margin-bottom: 32px;
      }

      .pricing-header h3 {
        font-size: 28px;
        font-weight: 700;
        color: var(--black);
        margin-bottom: 12px;
      }

      .pricing-amount {
        font-size: 62px;
        font-weight: 800;
        color: var(--green);
        line-height: 1;
      }

      .pricing-period {
        font-size: 19px;
        color: var(--dim-grey);
        margin-bottom: 24px;
      }

      .pricing-description {
        font-size: 16px;
        color: var(--dim-grey);
        line-height: 1.8;
        margin-bottom: 36px;
      }

      .pricing-features {
        list-style: none;
        margin-bottom: 36px;
      }

      .pricing-features li {
