    <script is:inline>
      var input = $("#Phone-Number-2")[0];
      let iti = window.intlTelInput(input, {
        initialCountry: "auto",
        onlyCountries: ["us", "ca", "gb", "in", "au"],
        useFullscreenPopup: false,
        countrySearch: false,
        geoIpLookup: function (callback) {
          $(".iti").css("width", "100%");
          $.get("https://ipinfo.io", function () {}, "jsonp").always(function (
            resp
          ) {
            var countryCode = resp && resp.country ? resp.country : "us";
            callback(countryCode);
          });
        },
        utilsScript: "npm/intl-tel-input-19.5.7/build/js/utils.js",
      });
      $("#wf-form-International-Contact-Form").submit(function (e) {
        e.preventDefault();
        var phoneNumber = input.value.replace(/[\s()-]+/g, "");
        var selectedCountryData = iti.getSelectedCountryData();
        if (!selectedCountryData.dialCode) {
          // Reinitialize intlTelInput to add auto country code
          iti.setCountry("us");
          selectedCountryData = iti.getSelectedCountryData();
        }
        var countryCode = "+" + selectedCountryData.dialCode;
        input.value = countryCode + phoneNumber;
        this.submit();
        return false; //I put it here as a fallback
      });
    </script>
